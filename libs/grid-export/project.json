{
  "name": "grid-export",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/grid-export/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/grid-export",
        "format": [
          "esm",
          "cjs"
        ],
        "main": "libs/grid-export/src/index.ts",
        "tsConfig": "libs/grid-export/tsconfig.lib.json",
        "assets": [
          {
            "glob": "*.mdx",
            "input": "libs/grid-export",
            "output": "."
          },
          {
            "glob": "LICENSE",
            "input": ".",
            "output": "."
          }
        ],
        "project": "libs/grid-export/package.json",
        "buildableProjectDepsInPackageJsonType": "peerDependencies",
        "generateExportsField": true,
        "rollupConfig": [
          "./libs/grid-react/rollup.config.js"
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs grid-export {args.ver} {args.tag}"
      },
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/grid-export/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/grid-export/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "scope:grid-export"
  ]
}